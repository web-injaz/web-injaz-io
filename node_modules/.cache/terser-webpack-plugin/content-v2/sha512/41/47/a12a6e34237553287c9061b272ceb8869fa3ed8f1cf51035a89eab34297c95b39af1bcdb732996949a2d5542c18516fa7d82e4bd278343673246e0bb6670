Reducer,\\r\\n    form: formReducer\\r\\n});\\r\\n\\r\\nexport default rootReducer;\",\"import React from 'react';\\nimport ReactDOM from 'react-dom';\\nimport { BrowserRouter, Switch, Route } from 'react-router-dom';\\n\\nimport App from './App';\\nimport BuildTool from './BuildTool';\\n\\nimport * as serviceWorker from './serviceWorker';\\n\\nimport { Provider } from 'react-redux';\\nimport { createStore, applyMiddleware } from 'redux';\\n\\n// import logger from 'redux-logger';\\nimport thunk from 'redux-thunk';\\nimport rootReducer from './reducers/index';\\n\\nconst store = createStore(rootReducer, applyMiddleware(thunk));\\n\\nReactDOM.render((\\n    <Provider store={store}>\\n        <BrowserRouter>\\n            <Switch>\\n                <Route exact path=\\\"/\\\" component={App} />\\n                <Route exact path=\\\"/build\\\" component={BuildTool} />\\n            </Switch>\\n        </BrowserRouter>\\n    </Provider>\\n), document.getElementById('root'));\\n\\n// If you want your app to work offline and load faster, you can change\\n// unregister() to register() below. Note this comes with some pitfalls.\\n// Learn more about service workers: http://bit.ly/CRA-PWA\\nserviceWorker.unregister();\\n\"]}","code":"(window.webpackJsonp=window.webpackJsonp||[]).push([[0],{131:function(e,a,t){var n=t(366);function r(e,a){return n(e)?\"\":\"@import 'node_modules/wi-framework/components/bootstrap/\".concat(a,\"';\")}function l(e,a){return n(e)?\"\":\"@import 'node_modules/wi-framework/components/\".concat(a,\"';\")}function s(e,a){return n(a)?\"\":\"$enable-\".concat(e,\": \").concat(a,\";\")}function c(e,a){return n(a)?\"\":\"$\".concat(e,\": \").concat(a,\";\")}e.exports=function(e){return c(\"primary\",e.primary)+c(\"secondary\",e.secondary)+s(\"default-root\",e.enableRoot)+s(\"inverse\",e.enableInverse)+s(\"darken\",e.enableDarken)+s(\"lighten\",e.enableLighten)+s(\"contrast\",e.enableContrast)+s(\"trans\",e.enableTrans)+s(\"gray\",e.enableGray)+\"@impor